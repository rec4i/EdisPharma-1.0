                    url: 'Ürün-Çalışma-Notları.aspx/Sunum_Başlık',
                    dataType: 'json',
                    type: 'POST',
                    async: false,
                    contentType: 'application/json; charset=utf-8',
                    success: function (data) {
                        var temp = JSON.parse(data.d);

                        for (var i = 0; i < temp.length; i++) {
                            var variable = '<div class="col-xs-12">' +
                                ' <div class="box box-primary">' +
                                '                        <div class="box-header with-border">' +
                                '                            <h2 class="box-title">'+temp[i].Sunum_Başlık+'</h2>' +
                                '                        </div>' +
                                '                        <!-- /.box-header -->' +
                                '                        <!-- form start -->' +
                                '                        <div class="box-body" Baslık_Id="' + temp[i].ID+'" id="Sunumlar_Eklenecek_Body">' +
                                '                        </div>' +
                                '                    </div>' +
                                '                    </div>' +
                                '';

                            $('#Başlıklar').append(variable)
                        }

                    },
                    error: function () {
                        alert('Talep esnasında sorun oluştu.Yeniden deneyin');
                    }
                });
                $.ajax({
                    url: 'Ürün-Çalışma-Notları.aspx/Sunum_İçerik',
                    dataType: 'json',
                    type: 'POST',
                    async: false,
                    contentType: 'application/json; charset=utf-8',
                    success: function (data) {
                        var temp = JSON.parse(data.d);

                        console.log(temp)
                        for (var i = 0; i < temp.length; i++) {
                            
                            var myvar = '<div class="col-sm-6 col-xs-12">' +
                                '        <div class="info-box">' +
                                '            <span class="info-box-icon"><a class="word" href="//docs.google.com/gview?url=http://176.236.238.166/Sunumlar/' + temp[i].Urun_Pdf_Link + '&embedded=true"">' +
                                '                <img style="height: 90px; width: 90px; border-radius: 2px; margin-top: -10px" src="' + temp[i].Urun_Resim.replace('~', '') + '" /></a></span>' +
                                '            <div class="info-box-content">' +
                                '                <span class="info-box-text" style="font-size: 18px">' + temp[i].Urun_Adı + '</span>' +
                                '                <span class="info-box-number" style="padding-top: 10px"><a href="http://176.236.238.166/Sunumlar/' + temp[i].Urun_Pdf_Link + '">Sunumu İndirmek İçin Tıklayın' +
                                '                </a></span>' +
                                '                <span class="info-box-number" style="font-size: 12px; padding-top: 2px;">Önzileme İçin Resime Tıklayın</span>' +
                                '            </div>' +
                                '            <!-- /.info-box-content -->' +
                                '        </div>' +
                                '        <!-- /.info-box -->' +
                                '    </div>';
                            $('div[id=Sunumlar_Eklenecek_Body][Baslık_Id=' + temp[i].Sunum_Başlık_Id + ']').append(myvar);
                            $(".word").fancybox({
                                'width': 1400, // or whatever
                                'height': 1000,
                                'type': 'iframe'
                            });
                        }
                        
                    },
                    error: function () {
                        alert('Talep esnasında sorun oluştu.Yeniden deneyin');
                    }
                });

                
            })
            
            
            

        })
    </script>

