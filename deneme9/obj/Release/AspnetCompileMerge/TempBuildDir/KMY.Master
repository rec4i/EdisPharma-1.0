                    success: function (data) {

                        for (var i = 0; i < data.d.split('!').length; i++) {// '<td><span class="label label-waring">Ziyret bekleniyor</span></td>'

                            var Header_Gun_span = $('span[id=Header_Gun_span_' + parseInt(data.d.split('!')[i].split('/')[0]) + ']')
